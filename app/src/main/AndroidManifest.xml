<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="willybrodus.binar_test">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".menu.IsiMenuActivity"
            android:label="Isi Menu" />

        <activity
            android:name=".menu.ListMenuActivity"
            android:label="List Menu" />

        <activity
            android:name=".order_menu.IsiOrder_menuActivity"
            android:label="Isi Order" />

        <activity
            android:name=".order_menu.ListOrder_menuActivity"
            android:label="List Order" />

        <activity
            android:name=".resto.IsiRestoActivity"
            android:label="Isi Resto" />

        <activity
            android:name=".resto.ListRestoActivity"
            android:label="List Resto" />

        <activity
            android:name=".user_app.IsiUser_appActivity"
            android:label="Isi User" />

        <activity
            android:name=".user_app.ListUser_appActivity"
            android:label="List User App" />
    </application>



</manifest>